{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": true,
    "babelOptions": {
      "configFile": "./.babelrc"
    }
  },
  "plugins": [
    "node"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "indent": ["error", 2],
    "import/no-named-as-default": ["off"],
    "import/no-named-as-default-member": ["off"]
  }
}
